<?xml version="1.0" encoding="utf-8"?>

<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>SharpMTProto</id>
    <version>$version$</version>
    <title>SharpMTProto</title>
    <authors>Alexander Logger</authors>
    <owners>Alexander Logger</owners>
    <licenseUrl>https://github.com/inTagger/SharpMTProto/blob/master/LICENSE.md</licenseUrl>
    <projectUrl>https://github.com/inTagger/SharpMTProto</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      MTProto (https://core.telegram.org/mtproto) client library.
	  
      Supported platforms:
        .NET Framework 4.5
    </description>
    <dependencies>
      <dependency id="SharpTL" version="0.9.0" />
      <dependency id="Rx-Main" version="2.2.5" />
      <dependency id="Nito.AsyncEx" version="3.0.0" />
      <dependency id="Raksha" version="0.3" />
      <dependency id="System.Collections.Immutable" version="1.1.33-beta" />
	  <dependency id="Microsoft.Tpl.Dataflow" version="4.5.24" />
    </dependencies>
  </metadata>
  <files>
    <file src="SharpMTProto.NET45\bin\$config$\SharpMTProto.dll" target="lib\net45\" />
    <file src="SharpMTProto.NET45\bin\$config$\SharpMTProto.pdb" target="lib\net45\" />
    <file src="SharpMTProto.NET45\bin\$config$\SharpMTProto.XML" target="lib\net45\" />
    <file src="SharpMTProto.NET45\**\*.cs" target="src\SharpMTProto.NET45\" exclude="**\obj\**" />
    <file src="SharpMTProto.Shared\**\*.cs" target="src\SharpMTProto.Shared\" />
  </files>
</package>
